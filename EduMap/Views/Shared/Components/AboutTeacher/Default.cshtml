@model IEnumerable<AboutTeacher>



@foreach (AboutTeacher aboutTeacher in Model)
{
    <div class="col-md-3 col-sm-4 col-xs-12">
        <div class="single-teacher">
            <div class="single-teacher-img">
                <a href="teacher-details.html"><img src="~/assest/img/teacher/@aboutTeacher.Image" alt="teacher"></a>
            </div>
            <div class="single-teacher-content text-center">
                <h2><a href="teacher-details.html">@aboutTeacher.FullName</a></h2>
                <h4>@aboutTeacher.Profession</h4>
                <ul>
                    <li><a href="@aboutTeacher.FacebookLink"><i class="zmdi zmdi-facebook"></i></a></li>
                    <li><a href="@aboutTeacher.PinterestLink"><i class="zmdi zmdi-pinterest"></i></a></li>
                    <li><a href="@aboutTeacher.VimeoLink"><i class="zmdi zmdi-vimeo"></i></a></li>
                    <li><a href="@aboutTeacher.TwitterLink"><i class="zmdi zmdi-twitter"></i></a></li>
                </ul>
            </div>
        </div>
    </div>
}




