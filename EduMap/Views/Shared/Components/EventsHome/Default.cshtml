

@model IEnumerable<EventsHome>
@foreach (EventsHome eventsHome in Model)
{
    <div class="col-md-6 col-sm-12 col-xs-12">


        <div class="single-event mb-35">


            <div class="event-date">
                <h3><a href="event-details.html">@eventsHome.DataMonth </a></h3>
            </div>
            <div class="event-content text-left">
                <div class="event-content-left">
                    <h4><a href="event-details.html">@eventsHome.Title</a></h4>
                    <ul>
                        <li><i class="fa fa-clock-o"></i>@eventsHome.DateTime</li>
                        <li><i class="fa fa-map-marker"></i>@eventsHome.Location</li>
                    </ul>
                </div>
                <div class="event-content-right">
                    <a class="default-btn" href="event-details.html">join now</a>
                </div>
            </div>

        </div>

    </div>
}




